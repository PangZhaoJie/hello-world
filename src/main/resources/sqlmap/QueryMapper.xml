<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.Mapper.QueryMapper">

    <select id="findById" resultType="String">
        SELECT
        userName
        FROM USER
        WHERE 1=1
        AND id = #{id}
    </select>


</mapper>
